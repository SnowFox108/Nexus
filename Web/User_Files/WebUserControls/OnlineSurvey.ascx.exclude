                    ControlToValidate="tbx_ContactEmail" Display="Dynamic" ValidationGroup="Question1">*</asp:RequiredFieldValidator>
                <asp:RegularExpressionValidator ID="RegularExpressionValidator1" runat="server" ControlToValidate="tbx_ContactEmail"
                    Display="Dynamic" ErrorMessage="Please enter a valid Email" ValidationExpression="\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*"
                    ValidationGroup="Question1">*</asp:RegularExpressionValidator>
            </div>
            <br />
