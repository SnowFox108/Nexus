                <asp:RequiredFieldValidator ID="RequiredFieldValidator1" runat="server" ErrorMessage="Please enter a Company Name"
                    ControlToValidate="tbx_CompanyName" Display="Dynamic" ValidationGroup="Question1">*</asp:RequiredFieldValidator>
            </div>
            <br />
            <div>
                <strong>2. Contact Email :</strong>
                <asp:TextBox ID="tbx_ContactEmail" runat="server" MaxLength="250" Width="300px" ValidationGroup="Question1"></asp:TextBox>
